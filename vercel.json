{
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "trailingSlash": true,
  "cleanUrls": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
